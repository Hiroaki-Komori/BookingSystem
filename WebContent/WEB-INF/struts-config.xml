<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
	"http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>
	<form-beans>
	<form-bean
		name="reserve"
		type="jp.level_five.bookingsystem.ReserveForm">
		<form-property name="title" type="java.lang.String"/>
		<form-property name="name" type="java.lang.String"/>
		<form-property name="month" type="int" />
		<form-property name="day" type="int" />
		<form-property name="hourFrom" type="int" />
		<form-property name="minFrom" type="int" />
		<form-property name="hourTo" type="int" />
		<form-property name="minTo" type="java.lang.String"/>
		</form-bean>
	</form-beans>

	<action-mappings>
	<action
		name="reserve"
		path="/reserve"
		type="jp.level_five.bookingsystem.ReserveAction"
		input="/reserve.jsp"/>
	</action-mappings>
</struts-config>